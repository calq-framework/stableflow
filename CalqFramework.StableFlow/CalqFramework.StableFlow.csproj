<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <RootNamespace>CalqFramework.StableFlow</RootNamespace>
    <PackageId>CalqFramework.StableFlow</PackageId>
    <Version>0.0.0</Version>
    <PackAsTool>true</PackAsTool>
    <ToolCommandName>stableflow</ToolCommandName>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Ghbvft6.Calq.Terminal" Version="0.*" />
    <PackageReference Include="Ghbvft6.Calq.Tooler" Version="0.*" />
  </ItemGroup>
</Project>